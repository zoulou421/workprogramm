<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Module Category Definition -->
    <record id="workprogramm_module_category" model="ir.module.category">
        <field name="name">Workflow Management</field>
        <field name="description">Module for managing workflow hierarchy and work programs</field>
    </record>

    <!-- User Groups Definition -->
    <record id="workprogramm_group_user" model="res.groups">
        <field name="name">Workflow User</field>
        <field name="category_id" ref="workprogramm_module_category"/>
        <field name="comment">Grants basic access to workflow data</field>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="workprogramm_group_manager" model="res.groups">
        <field name="name">Workflow Manager</field>
        <field name="category_id" ref="workprogramm_module_category"/>
        <field name="comment">Grants permissions to manage workflow data</field>
        <field name="implied_ids" eval="[(4, ref('workprogramm_group_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>

    <record id="workprogramm_group_admin" model="res.groups">
        <field name="name">Workflow Administrator</field>
        <field name="category_id" ref="workprogramm_module_category"/>
        <field name="comment">Grants full access to workflow data</field>
        <field name="implied_ids" eval="[(4, ref('workprogramm_group_manager'))]"/>
    </record>

    <!-- Access Rights for Workflow Hierarchy -->
